app_identifier ENV['IOS_APP_ID'] # The bundle identifier of your app
apple_id ENV['IOS_APPLE_USERNAME'] # Your Apple email address

team_id ENV['IOS_APPLE_TEAM_ID'] # Developer Portal Team ID

# you can even provide different app identifiers, Apple IDs and team names per lane:
# More information: https://github.com/fastlane/fastlane/blob/master/fastlane/docs/Appfile.md
